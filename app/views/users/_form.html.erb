<%= form_with model: user, url: form_path do |form| %> 
  <div class="form-group">
    <%= form.label :email %>
    <%= form.email_field :email %>
  </div>

  <div class="form-group">
    <%= form.label :password %>
    <%= form.password_field :password %>
  </div>

  <%= form.submit submit_button, class: "form-container_submit" %>
<% end %>

